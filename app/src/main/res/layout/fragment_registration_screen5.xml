<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="view.fragments.registrationScreen.RegistrationScreen5">

    <TextView
        android:id="@+id/registrationText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="80dp"
        android:fontFamily="@font/rubik_regular"
        android:gravity="center_vertical"
        android:text="Регистрация"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/registrationWelcomeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="110dp"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/rubik_regular"
        android:gravity="center_vertical"
        android:text="Ну что же, начнем?"
        android:textSize="32sp" />

    <com.magicgoop.tagsphere.TagSphereView
        android:id="@+id/tagView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/registrationButtonFrag5"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="30dp"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_marginBottom="30dp"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="@color/colorMainBlue"
        android:fontFamily="@font/rubik_regular"
        android:onClick="onClick"
        android:text="Поехали"
        android:textColor="@color/colorMainWhite"
        app:cornerRadius="15dp"
        app:strokeColor="@color/colorMainBlue"
        app:strokeWidth="2dp" />
</FrameLayout>
